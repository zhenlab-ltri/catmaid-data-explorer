{
  "name": "prototype-cde",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-js": "esbuild client/index.tsx --bundle --outfile=dist/index.js '--define:process.env.NODE_ENV=\"development\"'",
    "build-css": "esbuild client/css/index.css --bundle --loader:.woff2=dataurl --outfile=dist/index.css",
    "build-dev": "concurrently --kill-others \"npm run build-js\" \"npm run build-css\"",
    "build-prod": "esbuild client/index.tsx --bundle --outfile=dist/index.js --minify '--define:process.env.NODE_ENV=\"production\"'",
    "start": "node ./server/index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "esbuild": "^0.8.3",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "chart.js": "^2.9.4",
    "chroma-js": "^2.1.0",
    "cytoscape": "^3.16.1",
    "express": "^4.17.1",
    "lodash.countby": "^4.6.0",
    "lodash.debounce": "^4.0.8",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.9.0",
    "react-hover": "^2.0.0",
    "react-hyperscript": "^3.2.0",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-tabs": "^3.1.1",
    "react-virtualized": "^9.22.2"
  }
}
